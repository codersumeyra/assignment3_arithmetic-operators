package assignment3_ArithmeticOperators;

public class Question1 {

	public static void main(String[] args) {
		
		double gallons=15;
        double liters=1;
		
		//1 gallon=3.78 liters
	
		liters=gallons*3.78;
		
		
		System.out.println(gallons+" gallons is "+ liters +" liters");
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
