package assignment3_ArithmeticOperators;

public class Question6 {

	public static void main(String[] args) {
		
		int nomociad=10000; //nomociad=number of milligrams of caffeine in a drink
		int drink=500;
		int lethaloverdose=nomociad/drink;
		
		
		System.out.println("it would take "+lethaloverdose+ " drinks for a lethal overdose");
		
		

	}

}
